{
  "name": "vue-breadcrumb",
  "version": "0.1.0",
  "description": "Breadcrumbs for Vue JS. It uses routes as the main source of the breadcrumb",
  "main": "dist/vue-breadcrumb.umd.js",
  "module": "dist/vue-breadcrumb.esm.js",
  "unpkg": "dist/vue-breadcrumb.min.js",
  "browser": {
    "./sfc": "src/components/VueBreadcrumb.vue"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.4.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "babel-eslint": "10.0.1",
    "eslint": "5.14.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-vue": "5.2.2",
    "extract-text-webpack-plugin": "3.0.0",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8"
  },
  "keywords": [
    "breadcrumbs",
    "vue-breadcrumbs",
    "breadcrumb",
    "breadcrumb-navigation",
    "vue",
    "vuejs"
  ],
  "files": [
    "dist/*",
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ratscorp/vue-breadcrumb.git"
  },
  "author": "Gor Kotikyan <gkotikyan@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ratscorp/vue-breadcrumb/issues"
  },
  "homepage": "https://github.com/ratscorp/vue-breadcrumb#readme"
}
